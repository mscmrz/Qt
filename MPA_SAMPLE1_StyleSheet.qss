        selection-background-color: transparent;
        color: #AFBDC4;
        selection-color: #FFFFFF;
        background-color: #2c3b42;
        border:
}

QComboBox:disabled {
    color: #546E7A;
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
        color:white;
        background-color: transparent;
        selection-background-color: transparent;

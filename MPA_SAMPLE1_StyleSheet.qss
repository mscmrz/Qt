/************

QPUSHBUTTON

*************/
QPushButton{

      padding:5px;
      margin:3px;
      text-align: center;
      color: #fff;
      background-color:transparent;
      border-radius: 10px;
      border:2px solid #234a46;
}

QPushButton:hover{
      border: 2px inset #4bb856;
      background-color:transparent;
      border-top-style:none;
      border-bottom-style:none;
      border-right-style:solid;
      border-left-style:solid;

      }

QPushButton:pressed{

border: 2px solid  #234a46;
background-color:transparent;

     }

QPushButton:disabled{

      padding:7px;
      color:white;
      border-radius: 15px;
      border: 1px inset #234a46;
      background-color:rgba(129, 139, 140,0.3);

}

QPushButton:checked{

      padding:7px;
      color:white;
      border: 1px inset red;
      border-radius: 8px;
      background-color: #151f24;

}
QPushButton:checked:hover{

      padding:7px;
      color:white;
      border: 1px inset red;
      border-radius: 8px;
      background-color:transparent;

}



QPushButton:flat{
       padding:0px;
       border:2px solid #234a46;
       background-color:transparent;

}
QPushButton:flat:hover{
      border: 2px inset #4bb856;
      background-color:transparent;
      border-top-style:none;
      border-bottom-style:none;
      border-right-style:solid;
      border-left-style:solid;
            }

QPushButton:flat:pressed{
      border: 2px solid  #234a46;
      background-color:transparent;
            }


